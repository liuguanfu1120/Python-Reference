    "import pandas as pd\n",
    "import numpy as np\n",
    "file1 = '物天长跑队2021.10线上打卡.xlsx'\n",
    "df1 = pd.read_excel(file1)\n",
    "name1 = df1.iloc[:,0]\n",
    "weights = df1.iloc[:,1]/np.sum(df1.iloc[:,1])\n",
    "num = 5 #num是获奖的人数\n",
    "print('获奖的幸运儿是：',np.random.choice(name1,size=num,replace=False, p=weights))"
   ]
